{
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["ES2019", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "incremental": true,
    "outDir": "./.ts-cache/",
    "skipLibCheck": true,
    "strictNullChecks": true
  },
  "include": ["./src/**/*"]
}
